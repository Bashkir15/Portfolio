<!DOCTYPE html>
<html>
	<head>
		<meta charset='utf-8'>
		<title>Forrest Collins Skills</title>

		<link rel='stylesheet' href='./vendor/font-awesome/css/font-awesome.css'>
		<link rel='stylesheet' href='./vendor/etlinefont-bower/style.css'>
		<link href='https://fonts.googleapis.com/css?family=Muli:400,400italic|Playfair+Display:400,700|Lato:300italic,300|Montserrat:400,700' rel='stylesheet'>
		<link rel='stylesheet' href='./main.min.css'>
	</head>

	<body>
		<nav class='nav'>
			<div class='nav-left'>
				<div class='nav-item'>
					<h3>Forrest Collins</h3>
				</div>
			</div>


			<div class='nav-right'>
				<div class='nav-item'>
					<button id='open-sidenav'>
						<i class='icon icon-layers'></i>
					</button>
				</div>
			</div>
		</nav>

		<div id='sidenav-container' class='sidenav-container'>
			<div id='sidenav' class='sidenav'>
				<% include ../shared/sidenav %>
			</div>
		</div>

		<section id='skills-page-header' class='column column-center'>
			<div class='row'>
				<button id='frontend-trigger'>
					<span>Front-End</span>
				</button>

				<button id='backend-trigger'>
					<span>Backend</span>
				</button>

				<button id='efficiency-trigger'>
					<span>Efficiency</span>
				</button>

				<button>
					<span>UI/UX</span>
				</button>
			</div>
		</section>

		<section id='skills-front-end'>
			<% include ./frontend %>
		</section>

		<section id='skills-back-end'>
			<% include ./backend %>
		</section>

		<section id='skills-efficiency'>
			<% include ./efficiency %>
		</section>

		<section id='footer'>
			<% include ../shared/footer %>
		</section>



		<script src='./bundle.js'></script>
	</body>
</html>